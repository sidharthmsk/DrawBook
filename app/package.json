{
  "name": "drawbook",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "build:electron": "tsc -p electron/tsconfig.json && cp electron/preload.cjs dist/electron/preload.cjs",
    "start": "node dist/server/index.js",
    "electron:dev": "npm run build && npm run build:electron && electron .",
    "electron:pack": "npm run build && npm run build:electron && electron-builder --mac --dir",
    "electron:dist": "npm run build && npm run build:electron && electron-builder --mac",
    "electron:publish": "npm run build && npm run build:electron && electron-builder --mac --publish always"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.991.0",
    "@blocknote/core": "^0.46.2",
    "@blocknote/mantine": "^0.46.2",
    "@blocknote/react": "^0.46.2",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/language": "^6.12.1",
    "@codemirror/lint": "^6.9.4",
    "@codemirror/search": "^6.6.0",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.15",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@excalidraw/excalidraw": "^0.18.0",
    "@excalidraw/mermaid-to-excalidraw": "^2.0.0",
    "@mantine/core": "^8.3.15",
    "@mantine/hooks": "^8.3.15",
    "@tanstack/react-table": "^8.21.3",
    "@types/dagre": "^0.7.53",
    "@uiw/codemirror-themes": "^4.25.5",
    "@uiw/react-codemirror": "^4.25.5",
    "@univerjs/preset-sheets-core": "^0.15.5",
    "@univerjs/presets": "^0.15.5",
    "@xyflow/react": "^12.10.1",
    "adm-zip": "^0.5.16",
    "ag-grid-community": "^35.1.0",
    "ag-grid-react": "^35.1.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "codemirror": "^6.0.2",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "dotenv": "^17.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "groq-sdk": "^0.18.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "pdfjs-dist": "^5.4.624",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "tldraw": "^2.4.0",
    "uuid": "^13.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
